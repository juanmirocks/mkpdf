{
  "name": "parcel-reporter-mkpdf",
  "version": "1.0.0-wip",
  "description": "Parcel plugin to print a PDF out of a successfully built html, using Google's puppeteer",
  "private": true,
  "type": "commonjs",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "targets": {
    "main": {
      "optimize": true
    }
  },
  "scripts": {
    "clean": "rm  dist/* 2> /dev/null || true",
    "watch": "parcel watch",
    "build": "parcel build",
    "check": "tsc --noEmit",
    "check-watch": "npm run check -- -watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "parcel": "2.x"
  },
  "dependencies": {
    "puppeteer": "18.1.x"
  },
  "peerDependencies": {
    "@parcel/plugin": "^2.8.3",
    "@parcel/types": "^2.8.3"
  },
  "devDependencies": {
    "@parcel/transformer-typescript-tsc": "^2.8.3",
    "@tsconfig/recommended": "^1.0.2",
    "parcel": "^2.8.3",
    "typescript": "^4.9.5"
  }
}
